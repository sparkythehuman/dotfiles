{
	"editor.tabSize": 2,
	"editor.wordWrap": "wordWrapColumn",
	"[json]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2
	},
	"files.exclude": {
			"*.swo": true,
			"*.swp": true
	},
	"vim.hlsearch": true,
	"git.autofetch": true,
	"editor.fontSize": 15,
	"terminal.external.osxExec": "iTerm.app",
	"terminal.integrated.fontSize": 14,
	"editor.wordWrapColumn": 120,
	"editor.quickSuggestions": {
		"comments": false
	},
	"php.suggest.basic": false,
	"[php]": {
			"editor.defaultFormatter": "valeryanm.vscode-phpsab"
	},
	"[python]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 4,
		"editor.formatOnType": true
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"sqltools.useNodeRuntime": true,
	"[yaml]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"editor.quickSuggestions": {
				"other": true,
				"comments": false,
				"strings": true
		},
		"editor.autoIndent": "full",
		"editor.renderWhitespace": "all",
	},
	"yaml.format.enable": true,
	"yaml.trace.server": "verbose",
	"yaml.customTags": [
		"!And",
		"!If",
		"!Not",
		"!Equals sequence",
		"!Or",
		"!FindInMap",
		"!Base64",
		"!Cidr",
		"!Ref",
		"!Sub",
		"!GetAtt",
		"!GetAZs",
		"!ImportValue",
		"!Select",
		"!Split",
		"!Join"
	],
	"json.schemas": [
			{
					"fileMatch": [
							"stack*.json"
					],
					"url": "https://s3.amazonaws.com/cfn-resource-specifications-us-east-1-prod/schemas/2.15.0/all-spec.json"
			}
	],
	"yaml.schemas": {
			"https://s3.amazonaws.com/cfn-resource-specifications-us-east-1-prod/schemas/2.15.0/all-spec.json": ["stack*.yaml"]
	},
	"editor.renderControlCharacters": false,
	"editor.renderWhitespace": "none",
	"kite.showWelcomeNotificationOnStartup": false,
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter-notebook"
	},
	"kite.codefinder.enableLineDecoration": false,
	"workbench.colorTheme": "Atom One Light",
	"cSpell.enabled": true,
	"cSpell.enableFiletypes": [
		"shellscript",
		"terraform"
	],
	"remote.SSH.maxReconnectionAttempts": 2,
	"sync-rsync.onSave": true,
	"sync-rsync.exclude": [
		".git",
		".vscode",
		"__pycache__/",
		"*.py[cod]",
		"*$py.class",
		"*.so",
		".Python",
		"build/",
		"develop-eggs/",
		"dist/",
		"downloads/",
		"eggs/",
		".eggs/",
		"lib/",
		"lib64/",
		"parts/",
		"sdist/",
		"var/",
		"wheels/",
		"pip-wheel-metadata/",
		"share/python-wheels/",
		"*.egg-info/",
		".installed.cfg",
		"*.egg",
		"MANIFEST",
		"*.manifest",
		"*.spec",
		"pip-log.txt",
		"pip-delete-this-directory.txt",
		"htmlcov/",
		".tox/",
		".nox/",
		".coverage",
		".coverage.*",
		".cache",
		"nosetests.xml",
		"coverage.xml",
