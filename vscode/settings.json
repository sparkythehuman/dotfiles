		".env",
		".venv",
		"env/",
		"venv/",
		"ENV/",
		"env.bak/",
		"venv.bak/",
		".spyderproject",
		".spyproject",
		".ropeproject",
		"/site",
		".mypy_cache/",
		".dmypy.json",
		"dmypy.json",
		".pyre/",
		"*.swp",
		"package-lock.json",
